<script>
    $(document).ready(() => {
        $("#formEditar").submit(function(event){
            event.preventDefault();            
            $.ajax({
                type: "post",
                url: "/alunos/cadastro/edit",
                data: $(this).serialize(),
                success: function(data){          
                    console.log(data)                      
                    $("#cadastroSucesso").modal('show')
                }
            })
        })
    })   
</script>

