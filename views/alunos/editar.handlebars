<h3 style="color: darkblue;" class="mb-4">Edição de aluno</h3>

<form id="formEditar">
    <input type="hidden" name="idAluno" id="idAluno" value="{{aluno._id}}">
    <div class="card">        
        <div class="card-body">
            <h5 class="card-title" style="color: darkblue;">Informações do aluno</h5>
            <hr>
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label for="nomeAluno">Nome</label>
                        <input type="text" class="form-control" id="nomeAluno" name="nomeAluno" placeholder="Nome do aluno" value="{{aluno.nome}}" required>
                    </div>
                </div>
                <div class="col-sm-2">
                    <div class="form-group">
                        <label for="dataNascAluno">Nascimento</label>
                        <input type="date" class="form-control" id="dataNascAluno" name="dataNascAluno">
                    </div>
                </div>                
                <div class="col-sm-2">
                    <div class="form-group">
                        <label for="NISAluno">NIS</label>
                        <input type="text" class="form-control" id="NISAluno" name="NISAluno" placeholder="NIS" value="{{aluno.NIS}}" required>
                    </div>
                </div>
                <div class="col-sm-2">
                    <div class="form-group">
                        <label for="idCensoAluno">ID Censo</label>
                        <input type="text" class="form-control" id="idCensoAluno" name="idCensoAluno" placeholder="ID Censo" value="{{aluno.IDCenso}}" required>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-2">
                    <div class="form-group">
                        <label for="sexoAluno">Sexo</label>
                        <select class="form-control" id="sexoAluno" name="sexoAluno">
                            <option value="MASCULINO" {{#if_equal aluno.sexo "MASCULINO"}}selected{{/if_equal}}>MASCULINO</option>
                            <option value="FEMININO" {{#if_equal aluno.sexo "FEMININO"}}selected{{/if_equal}}>FEMININO</option>
                        </select>
                    </div>
                </div>
                <div class="col-sm-2">
                    <div class="form-group">
                        <label for="corAluno">Cor/Raça</label>
                        <select class="form-control" id="corAluno" name="corAluno">
                            <option value="BRANCA" {{#if_equal aluno.cor "BRANCA"}}selected{{/if_equal}}>BRANCA</option>
                            <option value="PRETA" {{#if_equal aluno.cor "PRETA"}}selected{{/if_equal}}>PRETA</option>
                            <option value="PARDA" {{#if_equal aluno.cor "PARDA"}}selected{{/if_equal}}>PARDA</option>
                            <option value="AMARELA" {{#if_equal aluno.cor "AMARELA"}}selected{{/if_equal}}>AMARELA</option>
                            <option value="INDÍGENA" {{#if_equal aluno.cor "INDÍGENA"}}selected{{/if_equal}}>INDÍGENA</option>
                            <option value="QUILOMBOLA" {{#if_equal aluno.cor "QUILOMBOLA"}}selected{{/if_equal}}>QUILOMBOLA</option>
                            <option value="NÃO DECLARADA" {{#if_equal aluno.cor "NÃO DECLARADA"}}selected{{/if_equal}}>NÃO DECLARADA</option>                            
                        </select>
                    </div>
                </div>
                <div class="col-sm-4">
                    <div class="form-group">
                        <label for="situacaoAluno">Situação do aluno</label>
                        <select class="form-control" id="situacaoAluno" name="situacaoAluno">
                            <option value="AGUARDANDO MATRÍCULA" {{#if_equal aluno.situacao "AGUARDANDO MATRÍCULA"}}selected{{/if_equal}}>Aguardando matrícula</option>
                            <option value="AGUADANDO TRANSFERÊNCIA" {{#if_equal aluno.situacao "AGUARDANDO TRANSFERÊNCIA"}}selected{{/if_equal}}>Aguardando transferência</option>
                            <option value="AGUARDANDO VAGA" {{#if_equal aluno.situacao "AGUARDANDO VAGA"}}selected{{/if_equal}}>Aguardando vaga</option>
                            <option value="INATIVO - ABANDONOU A ESCOLA" {{#if_equal aluno.situacao "INATIVO - ABANDONOU A ESCOLA"}}selected{{/if_equal}}>Inativo - Abandonou a escola</option>
                            <option value="INATIVO - CONCLUIU O CURSO" {{#if_equal aluno.situacao "INATIVO - CONCLUIU O CURSO"}}selected{{/if_equal}}>Inativo - Concluiu o curso</option>
                            <option value="INATIVO - DISPENSADO" {{#if_equal aluno.situacao "DISPENSADO"}}selected{{/if_equal}}>Inativo - Dispensado</option>
                            <option value="INATIVO - FALECIDO" {{#if_equal aluno.situacao "INATIVO - FALECIDO"}}selected{{/if_equal}}>Inativo - Falecido</option>                            
