                            <option value="INATIVO - TRANSFERIDO DE ESCOLA" {{#if_equal aluno.situacao "INATIVO - TRANSFERIDO DE ESCOLA"}}selected{{/if_equal}}>Inativo - Transferido da escola</option>
                            <option value="MATRICULADO" {{#if_equal aluno.situacao "MATRICULADO"}}selected{{/if_equal}}>Matriculado</option>
                            <option value="PRÉ-MATRÍCULA" {{#if_equal aluno.situacao "PRÉ-MATRÍCULA"}}selected{{/if_equal}}>Pré-matrícula</option>
                        </select>
                    </div>
                </div>
                <div class="col-sm-4">
                    <div class="form-group">
                        <label for="emailAluno">E-mail do aluno</label>
                        <input type="text" class="form-control" id="emailAluno" name="emailAluno" placeholder="E-mail do aluno" value="{{aluno.emailAluno}}" required>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-3">
                    <div class="form-group">
                        <label for="matricAluno">Matricula do aluno</label>
